.flickr-dialog-short-message .short-msg-success 	{ position: absolute; top: 5px; left: 10px; display: block; width: 19px; height: 16px; background: url(https://s.yimg.com/pw/images/iphone/sprite_confirmation_icons.png.v2) no-repeat 6px 3px; }
.flickr-dialog-short-message .short-msg-warning 	{ position: absolute; top: 5px; left: 10px; display: block; width: 20px; height: 17px; background: url(https://s.yimg.com/pw/images/iphone/sprite_confirmation_icons.png.v2) no-repeat -14px 3px; }
.flickr-dialog-short-message .short-msg-spinner 	{ position: absolute; top: 5px; left: 10px; display: block; width: 20px; height: 17px; background: url(https://s.yimg.com/pw/images/progress/balls-16x8-trans.gif) no-repeat 2px 6px; }

