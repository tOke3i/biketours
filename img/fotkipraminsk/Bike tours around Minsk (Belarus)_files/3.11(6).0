/* -------------------------------------------------------------------------------- */
/* Styles for all dialog types. */
/* -------------------------------------------------------------------------------- */

.flickr-dialog						{ position: fixed; top: 50%; left: 50%; z-index: 20001; width: 330px; margin: -90px 0 0 -165px; text-align: left; font-size: 12px; }
.flickr-dialog-mask					{ position: fixed; top: 0; left: 0; z-index: 20000; width: 100%; height: 100%; background-color: #000; opacity: .35; -moz-opacity: .35; filter: alpha(opacity=35); }
.flickr-dialog.hidden,
.flickr-dialog-mask.hidden				{ display: none; }

.flickr-dialog .flickr-dialog-container 		{ float: left; position: relative; z-index: 1; width: 100%; padding: 1px; background-color: #fff;
								-moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px;
								*outline: 4px solid #ccc; -moz-box-shadow: 0 0 8px rgba(0, 0, 0, 0.5); -webkit-box-shadow: 0 0 8px rgba(0, 0, 0, 0.5); box-shadow: 0 0 8px rgba(0, 0, 0, 0.5); }

.flickr-dialog .hd 					{ clear: both; overflow: hidden; padding: 10px; background: #f7f7f7; zoom: 1; min-height:18px; }
.flickr-dialog .hd h3 					{ margin-bottom: 0; margin-top: 0; font-size: 16px; font-weight: normal; height: 18px; color: #000;}
.flickr-dialog .bd 					{ clear: both; padding: 10px; background: #fff; }
.flickr-dialog .ft 					{ padding: 8px; background: #dce8f6;
								-webkit-border-bottom-right-radius: 4px; -webkit-border-bottom-left-radius: 4px;
								-moz-border-radius-bottomright: 4px; -moz-border-radius-bottomleft: 4px;
								border-bottom-right-radius: 4px; border-bottom-left-radius: 4px; }

.flickr-dialog .button-container 			{ text-align: center; }

/* -------------------------------------------------------------------------------- */
/* Spinner. */
/* -------------------------------------------------------------------------------- */

.flickr-dialog-spinner 					{  }
.flickr-dialog-spinner .flickr-dialog-container 	{ width: 24px; height: 12px; padding: 8px 16px; background: #fff url(https://s.yimg.com/pw/images/progress/balls-24x12-white.gif) no-repeat 50% 50%;
								-moz-border-radius: 13px; -webkit-border-radius: 13px; border-radius: 13px; *border: 1px solid #999;
								-moz-box-shadow: 0 0 6px rgba(0, 0, 0, 0.5); -webkit-box-shadow: 0 0 6px rgba(0, 0, 0, 0.5); box-shadow: 0 0 6px rgba(0, 0, 0, 0.5); }

