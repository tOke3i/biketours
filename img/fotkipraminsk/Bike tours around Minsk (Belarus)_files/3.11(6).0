div.global_dialog {
	position:absolute;
	position:fixed;
	_position:absolute;
	display:none;
	width:330px;
	text-align:left;
	font-size:12px;
	z-index: 20001;
}

div.global_dialog .global_dialog_container .bd {
	background:#fff;
	*zoom:1;
}

#global_dialog.organizr .global_dialog_container {
	/* 
	organizr-style dialog
	if adapting for multiple use case: .global-dialog.organizr
	*/
	border:9px solid #d6d6d6;
}

#global_dialog.organizr .global_dialog_container .wrapper {
	/* optional container for .hd, .bd, .ft - for border purposes */
	border:1px solid #c6c6c6;
}

#global_dialog.organizr .global_dialog_container .bd {
	border:1px solid #fff;
}

#global-dialog-background {
	display: none;
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 20000;
}

div.global_dialog_spinner {
	position:absolute;
	position:fixed;
	_position:absolute;
	left:0px;
	top:0px;
	width:100%;
	height:100%;
	top:50%;
	left:50%;
	margin-left:-20px;
	margin-top:-14px;
	cursor:wait;
	cursor:progress;
	background:#fff url(https://s.yimg.com/pw/images/progress/balls-24x12-white.gif) no-repeat 50% 50%;
	width:24px;
	_width:48px;
	height:12px;
	_height:28px;
	padding:8px 16px;
	-moz-border-radius:13px;
	-webkit-border-radius:13px;
	border-radius:13px;
	*border:1px solid #999;
	-moz-box-shadow:0px 0px 6px rgba(0,0,0,0.5);
	-webkit-box-shadow:0px 0px 6px rgba(0,0,0,0.5);
	box-shadow:0px 0px 6px rgba(0,0,0,0.5);
	z-index:20001;
}

div.global-dialog-short-msg {
	position: absolute;
	position: fixed;
	_position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	top: 50%;
	left: 50%;
	margin-left: -20px;
	margin-top: -14px;
	background: #fff;
	width: 24px;
	_width: 48px;
	height: 19px;
	_height: 31px;
	padding: 6px 15px;
	-moz-border-radius: 15px;
	-webkit-border-radius: 15px;
	border-radius: 13px;
	*border: 1px solid #999;
	-moz-box-shadow: 0 0 8px rgba(0,0,0,0.5);
	-webkit-box-shadow: 0 0 8px rgba(0,0,0,0.5);
	box-shadow: 0 0 8px rgba(0,0,0,0.5);
	z-index: 20001;
}

div.global_dialog .global_dialog_container {
	float: left;
	position: relative;
	z-index: 1;
	width: 300px;
	_width: 314px;
	padding: 1px;
	background-color: #fff;
	-moz-border-radius:4px;
	-webkit-border-radius:4px;
	border-radius:4px;
}

div.global-dialog-short-msg .global_dialog_container {
	position: relative;
	padding: 1px 1px 1px 22px;
}

div.global-dialog-short-msg span.short-msg-success {
	position:absolute;
	top:-1px;
	left:-5px;
	display:block;
	width:19px;
	height:16px;
	background:url(https://s.yimg.com/pw/images/iphone/sprite_confirmation_icons.png.v2) no-repeat 6px 3px;
}

div.global-dialog-short-msg span.short-msg-warning {
	position:absolute;
	top:-1px;
	left:-5px;
	display:block;
	width:20px;
	height:17px;
	background:url(https://s.yimg.com/pw/images/iphone/sprite_confirmation_icons.png.v2) no-repeat -14px 3px;
}

div.global_dialog .shadow_sprite {
	float:left;
	background:transparent url(https://s.yimg.com/pw/images/tc_white_shadow_sprite.png) no-repeat 0 0;
	line-height:1px;
	_visibility:hidden;
	padding:0;
}

div.global_dialog .shadow_tl {
	width:7px;
	height:6px;
	background-position:-1px -1px;
	overflow:hidden;
}

div.global_dialog .shadow_t {
	width:314px;
	height:6px;
	background-position:-11px 0;
	overflow:hidden;
}

div.global_dialog .shadow_tr {
	width:7px;
	height:6px;
	background-position:-992px -1px;
	overflow:hidden;
}

div.global_dialog .shadow_l {
	width:7px;
	height:381px;
	background-position:0 -11px;
	clear:left;
}

div.global_dialog .shadow_r {
	width:7px;
	height:381px;
	background-position:-993px -11px;
}

div.global_dialog .shadow_bl {
	width:7px;
	height:7px;
	background-position:-1px -992px;
	clear:left;
}

div.global_dialog .shadow_b {
	width:314px;
	height:7px;
	background-position:-11px -992px;
	margin-top:-1px;
}

div.global_dialog .shadow_br {
	width:7px;
	height:7px;
	background-position:-992px -992px;
}

div.global_dialog .hd {
	clear:both;
	overflow:hidden;
	zoom:1;
	background:#F7F7F7;
	padding:10px;
}

div.global_dialog .hd h3 {
	margin-bottom: 0;
	font-size: 15px;
}

div.global_dialog .bd {
	clear:both;
	background:#fff;
	padding:10px;
}

div.global_dialog .ft {
	background:#dce8f6;
	padding:8px;
	-webkit-border-bottom-right-radius: 4px;
	-webkit-border-bottom-left-radius: 4px;
	-moz-border-radius-bottomright: 4px;
	-moz-border-radius-bottomleft: 4px;
	border-bottom-right-radius: 4px;
	border-bottom-left-radius: 4px;
}

div.global_dialog .hd img {
	float:left;
	border:2px solid #fff;
	margin-right:8px;
}

div.global_dialog .hd div {
	overflow:visible;
	padding:4px;
}

div.global_dialog .button_container {
	border-top:1px solid #D7D7D7;
	text-align:center;
}

div.global_dialog .confirmation_button_container {
	border-top: none;
}

div.global_dialog .photo_container {
	padding-top: 13px;
	text-align: center;
	line-height: 0;
}

div.global_dialog .global_dialog_container {
	*outline:4px solid #ccc;
	-moz-box-shadow:0px 0px 8px rgba(0,0,0,0.5);
	-webkit-box-shadow:0px 0px 8px rgba(0,0,0,0.5);
	box-shadow:0px 0px 8px rgba(0,0,0,0.5);
}

div.global_dialog .dialog-loaded,
div.global_dialog .dialog-loading {
	display:none;
}

#global_dialog.loading .dialog-loading,
#global_dialog.loaded .dialog-loaded {
	display:block;
}

#global_dialog.loading .dialog-loading .bd {
	display:block;
	padding:5px;
	text-align:center;
	background-image:url(https://s.yimg.com/pw/images/iphone/balls-24x12-trans.gif);
	background-position:50% 50%;
	background-repeat:no-repeat;
	_height:15px;
	min-height:15px;
}

#global_dialog.loaded .hd,
#global_dialog.loaded .bd,
#global_dialog.loaded .ft {
	display:block;
}

/* Pre-sidecar, 2009 dialog style */

#global_dialog.style_2009 .global_dialog_container {
	/* old style, no border radius etc. */
	float: left;
	position: relative;
	z-index: 1;
	width: 300px;
	_width: 314px;
	overflow: hidden;
	padding: 1px;
	border: 6px solid #f7f7f7;
	background-color: #d7d7d7;
}
#global_dialog.style_2009 .global_dialog_container,
#global_dialog.style_2009 .global_dialog_container .ft {
	-moz-border-radius:0px;
	-webkit-border-radius:0px;
	-border-radius:0px;
}

/* greath auth dialog, here because it is small */
#global_dialog.auth .global_dialog_container{
	width: 360px;
	height: 223px;
}

#global_dialog.auth .wrapper .hd{
	background-color: #fff;
	border: none;
	margin-top: 40px;
	text-align: center;
}

#global_dialog.auth .wrapper .button_container{
	border-top: none;
}

#global_dialog.auth .wrapper .button_container .Butt{
	background-color: #bbb;
}

			
/*
 * hacky maximize code for bert
 */

#global_dialog.global-dialog-maximized {
	width: 100% !important;
	height: 100% !important;
	top: 0 !important;
	left: 0 !important;
	margin: 0 !important;
}

#global_dialog.global-dialog-maximized .global_dialog_container {
	width: 100% !important;
	height: 100% !important;
}

#global_dialog.global-dialog-maximized .global-dialog-admin-maximizer {
	display: none;
}
	
#global_dialog .global-dialog-admin-maximizer {
	height: 16px;
	background: #f7f7f7;
	text-align: center;
	font-size: 85%;
	clear: both;
}

/* -------------------------------------------------------------------------------- */
/* Styles for the Keyboard Shortcut Legend modal */
/* -------------------------------------------------------------------------------- */

.keyboard-shortcut-legend-hd								{ position: relative; font-size: 17px; line-height: 21px; }
.keyboard-shortcut-legend-hd h2								{ display: inline; font-size: 17px; line-height: 21px; }
.keyboard-shortcut-legend-close:link,
.keyboard-shortcut-legend-close:visited							{ position: absolute; top: 12px; right: 12px; width: 16px; height: 16px; background: url(https://s.yimg.com/pw/images/close_x_sprite.png) no-repeat 0 0; }
.keyboard-shortcut-legend-close:hover,
.keyboard-shortcut-legend-close:active							{ background-position: 0px -22px; }

.keyboard-shortcut-legend-hd .killswitch-container					{ position: absolute; right: 66px; width: 400px; font-size: 13px; text-align: right; }
.keyboard-shortcut-legend-hd .killswitch-container input.killswitch			{ margin-right: 6px; }
.keyboard-shortcut-legend-hd .spinner							{ display: none; width: auto; padding: 0 12px 0 22px; background: url(https://s.yimg.com/pw/images/progress/balls-16x8-trans.gif) no-repeat 0 6px; font-size: 11px; }
.keyboard-shortcut-legend-hd .success							{ display: none; padding: 0 12px 0 22px; background: none; font-size: 11px; }
.keyboard-shortcut-legend-hd .alert							{ display: none; padding: 0 12px 0 22px; background: none; font-size: 11px; }
.keyboard-shortcut-legend-hd .spinner.show,
.keyboard-shortcut-legend-hd .success.show,
.keyboard-shortcut-legend-hd .alert.show						{ display: inline-block; }
.keyboard-shortcut-legend-hd .success .msg-icon						{ display: inline-block; width: 18px; height: 15px; background: url(https://s.yimg.com/pw/images/flickr-sprite.png.v5) no-repeat -1533px -92px; vertical-align: sub; }
.keyboard-shortcut-legend-hd .alert .msg-icon						{ display: inline-block; width: 18px; height: 15px; background: url(https://s.yimg.com/pw/images/flickr-sprite.png.v5) no-repeat -1533px -12px; vertical-align: sub; }

/* -------------------------------------------------------------------------------- */
/* Keys */
/* -------------------------------------------------------------------------------- */

.keyboard-shortcut-legend-bd								{ position: relative; }
.keyboard-shortcut-legend-bd .keyboard-row						{ overflow: hidden; min-height: 48px; *height: 48px; margin: 0; padding: 4px 0 0 4px; }
.keyboard-shortcut-legend-bd .keyboard-row li						{ position: relative; float: left; width: 48px; height: 48px; list-style: none; margin-right: 4px; background: transparent; }

.keyboard-shortcut-legend-bd .keyboard-row span.key-container				{ display: block; position: absolute; top: 0; left: 0; width: 44px; height: 44px; 
	 											border-radius: 8px; -moz-border-radius: 8px; -webkit-border-radius: 8px;
												-moz-box-shadow: 0 0 4px #000 inset; -webkit-box-shadow: 0 0 4px #000 inset; box-shadow: 0 0 4px #000 inset; }

.keyboard-shortcut-legend-bd .keyboard-row a.key-height					{ display: block; position: absolute; width: 43px; height: 43px; margin: -1px 0 0 -1px; background: #d8d8d8; text-decoration: none; cursor: default;
	 											border-radius: 6px; -moz-border-radius: 6px; -webkit-border-radius: 6px;
												-moz-box-shadow: 1px 1px 4px #888; -webkit-box-shadow: 1px 1px 4px #888; box-shadow: 1px 1px 4px #888; }
                                                                        		
.keyboard-shortcut-legend-bd .keyboard-row span.key					{ display: block; width: 40px; height: 40px; background: #f3f3f3; color: #222;
												border-radius: 4px; -moz-border-radius: 4px; -webkit-border-radius: 4px; }
                                                                        		
.keyboard-shortcut-legend-bd .keyboard-row span.sup					{ display: block; position: absolute; top: 2px; left: 6px; }
.keyboard-shortcut-legend-bd .keyboard-row span.sub					{ display: block; position: absolute; bottom: 5px; left: 7px; }

/* -------------------------------------------------------------------------------- */
/* Keys (mini mode) */
/* -------------------------------------------------------------------------------- */

.keyboard-shortcut-legend-bd.mini .keyboard-row						{ min-height: 24px; *height: 24px; padding: 3px 0 0 98px; }
.keyboard-shortcut-legend-bd.mini .keyboard-row li					{ width: 24px; height: 24px; margin-right: 3px; }
                                                                        		
.keyboard-shortcut-legend-bd.mini .keyboard-row span.key-container			{ width: 22px; height: 22px; 
	 											border-radius: 4px; -moz-border-radius: 4px; -webkit-border-radius: 4px;
												-moz-box-shadow: 0 0 2px #000 inset; -webkit-box-shadow: 0 0 2px #000 inset; box-shadow: 0 0 2px #000 inset; }
                                                                        		
.keyboard-shortcut-legend-bd.mini .keyboard-row a.key-height				{ width: 22px; height: 22px; 
	 											border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px;
												-moz-box-shadow: 1px 1px 1px #888, -1px -1px 1px #cacaca; 
												-webkit-box-shadow: 1px 1px 1px #888, -1px -1px 1px #cacaca; 
												box-shadow: 1px 1px 1px #888, -1px -1px 1px #cacaca; }
                                                                        		
.keyboard-shortcut-legend-bd.mini .keyboard-row span.key				{ width: 20px; height: 20px;
												border-radius: 2px; -moz-border-radius: 2px; -webkit-border-radius: 2px; }
                                                                        		
.keyboard-shortcut-legend-bd.mini .keyboard-row span.sup				{ top: -4px; left: 3px; font-size: 7px; }
.keyboard-shortcut-legend-bd.mini .keyboard-row span.sub				{ bottom: -2px; left: 3px; font-size: 7px; }

/* -------------------------------------------------------------------------------- */
/* Default key styles for QWERTY */
/* -------------------------------------------------------------------------------- */

.keyboard-shortcut-legend-bd .keyboard-row li.esc					{ width: 51px; height: 31px; }
.keyboard-shortcut-legend-bd .keyboard-row li.esc span.key-container			{ width: 47px; height: 27px; }
.keyboard-shortcut-legend-bd .keyboard-row li.esc a.key-height				{ width: 46px; height: 26px; }
.keyboard-shortcut-legend-bd .keyboard-row li.esc span.key				{ width: 43px; height: 23px; }
                                                                        		
.keyboard-shortcut-legend-bd .keyboard-row li.func					{ width: 51px; height: 31px; }
.keyboard-shortcut-legend-bd .keyboard-row li.func span.key-container			{ width: 47px; height: 27px; }
.keyboard-shortcut-legend-bd .keyboard-row li.func a.key-height				{ width: 46px; height: 26px; }
.keyboard-shortcut-legend-bd .keyboard-row li.func span.key				{ width: 43px; height: 23px; font-size: 11px; }
                                                                        		
.keyboard-shortcut-legend-bd .keyboard-row li.delete					{ width: 90px; }
.keyboard-shortcut-legend-bd .keyboard-row li.delete span.key-container			{ width: 86px; }
.keyboard-shortcut-legend-bd .keyboard-row li.delete a.key-height			{ width: 85px; }
.keyboard-shortcut-legend-bd .keyboard-row li.delete span.key				{ width: 82px; }
                                                                        		
.keyboard-shortcut-legend-bd .keyboard-row li.return 					{ width: 108px; }
.keyboard-shortcut-legend-bd .keyboard-row li.return span.key-container			{ width: 104px; }
.keyboard-shortcut-legend-bd .keyboard-row li.return a.key-height			{ width: 103px; }
.keyboard-shortcut-legend-bd .keyboard-row li.return span.key				{ width: 100px; }
                                                                        		
.keyboard-shortcut-legend-bd .keyboard-row li.shift					{ width: 89px; }
.keyboard-shortcut-legend-bd .keyboard-row li.shift span.key-container			{ width: 85px; }
.keyboard-shortcut-legend-bd .keyboard-row li.shift a.key-height			{ width: 84px; }
.keyboard-shortcut-legend-bd .keyboard-row li.shift span.key				{ width: 81px; }
                                                                        		
.keyboard-shortcut-legend-bd .keyboard-row li.space					{ width: 308px; margin-right: 149px; }
.keyboard-shortcut-legend-bd .keyboard-row li.space span.key-container			{ width: 304px; }
.keyboard-shortcut-legend-bd .keyboard-row li.space a.key-height			{ width: 303px; }
.keyboard-shortcut-legend-bd .keyboard-row li.space span.key				{ width: 300px; }
                                                                        		
.keyboard-shortcut-legend-bd .keyboard-row li.right-arrow				{ margin-right: 0; }
                                                                        		
.keyboard-shortcut-legend-bd .keyboard-row li.small-text 				{ font-size: 9px; }

/* -------------------------------------------------------------------------------- */

.keyboard-shortcut-legend-bd .row-0							{ min-height: 31px; *height: 31px; }
.keyboard-shortcut-legend-bd .row-2							{ margin-left: 67px; }
.keyboard-shortcut-legend-bd .row-3							{ margin-left: 86px; }
.keyboard-shortcut-legend-bd .row-4							{ margin-left: 105px; }
.keyboard-shortcut-legend-bd .row-5							{ margin-left: 209px; }
                                                                        		
.keyboard-shortcut-legend-bd hr								{ margin-top: 12px; border: 1px dotted #ccc; border-width: 0 0 1px; }

/* -------------------------------------------------------------------------------- */
/* QWERTZ & AZERTY layout */
/* -------------------------------------------------------------------------------- */

.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return-top,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return-top 			{ width: 75px; }
.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return-top span.key-container,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return-top span.key-container { width: 71px; }
.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return-top a.key-height,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return-top a.key-height 	{ width: 70px; }
.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return-top span.key,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return-top span.key 	{ width: 67px; }

.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return-middle,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return-middle 		{ width: 56px; height: 12px; overflow: hidden; margin: 36px 0 0 -60px; background-color: #fff; }
.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return-middle span.key-container,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return-middle span.key-container { width: 52px; margin-top: -20px; }
.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return-middle a.key-height,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return-middle a.key-height 	{ width: 51px; }
.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return-middle span.key,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return-middle span.key 	{ width: 48px; }

.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return 			{ width: 56px; height: 58px; margin-top: -10px; }
.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return span.key-container,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return span.key-container 	{ width: 52px; height: 54px; }
.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return a.key-height,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return a.key-height		{ width: 51px; height: 53px; }
.keyboard-shortcut-legend-bd.qwertz-layout .keyboard-row li.return span.key,
.keyboard-shortcut-legend-bd.azerty-layout .keyboard-row li.return span.key 		{ width: 48px; height: 50px; }

/* -------------------------------------------------------------------------------- */

.keyboard-shortcut-legend-bd.qwertz-layout .row-4,
.keyboard-shortcut-legend-bd.azerty-layout .row-4					{ margin-left: 53px; }

